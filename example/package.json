{
  "name": "vue-ts",
  "private": true,
  "version": "0.0.0",
  "keywords": [
    "vue",
    "esbuild"
  ],
  "type": "module",
  "author": "SnowdreamTech Inc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowdreamtech/vue-ts.git"
  },
  "bugs": {
    "url": "https://github.com/snowdreamtech/vue-ts/issues"
  },
  "homepage": "https://github.com/snowdreamtech",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "stage": "vue-tsc && vite build --mode staging",
    "testing": "vue-tsc && vite build --mode testing",
    "preview": "vite preview",
    "lint": "pnpm lint:eslint && pnpm lint:stylelint",
    "lint:eslint": "eslint --fix --ext .js,.jsx,.vue,.ts,.tsx .",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss,sass,styl}\"",
    "lint:commitlint": "commitlint --edit"
    },
  "dependencies": {
    "@snowdreamtech/vue-tui-editor": "0.1.1",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@rushstack/eslint-patch": "1.5.0",
    "@types/node": "20.7.1",
    "@vitejs/plugin-vue": "4.3.4",
    "@vue/eslint-config-typescript": "12.0.0",
    "eslint": "8.50.0",
    "eslint-plugin-vue": "9.17.0",
    "husky": "8.0.3",
    "stylelint": "15.10.3",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recommended": "13.0.0",
    "stylelint-config-recommended-less": "2.0.0",
    "stylelint-config-recommended-scss": "13.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-standard-scss": "11.0.0",
    "stylelint-less": "2.0.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-stylelint": "5.1.1",
    "vue-tsc": "1.8.15"
  }
}
